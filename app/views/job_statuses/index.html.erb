<h1>Listing job_statuses</h1>

<table>
  <tr>
    <th>Description</th>
  </tr>

<% @job_statuses.each do |job_status| %>
  <tr>
    <td><%=h job_status.description %></td>
    <td><%= link_to 'Show', job_status %></td>
    <td><%= link_to 'Edit', edit_job_status_path(job_status) %></td>
    <td><%= link_to 'Destroy', job_status, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New job_status', new_job_status_path %>