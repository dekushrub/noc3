<h1>Listing job_materials</h1>

<table>
  <tr>
    <th>Job</th>
    <th>Material</th>
    <th>Quantity</th>
  </tr>

<% @job_materials.each do |job_material| %>
  <tr>
    <td><%=h job_material.job_id %></td>
    <td><%=h job_material.material_id %></td>
    <td><%=h job_material.quantity %></td>
    <td><%= link_to 'Show', job_material %></td>
    <td><%= link_to 'Edit', edit_job_material_path(job_material) %></td>
    <td><%= link_to 'Destroy', job_material, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New job_material', new_job_material_path %>