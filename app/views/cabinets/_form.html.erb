<% semantic_form_for @cabinet do |f| %>
<% f.inputs do %>
  <%= f.input :label, :label => 'Label' %>
  <%= f.input :building_id, :label => 'Building', :collection => Building.all %>
  <%= f.input :room, :label => 'Room' %>
  <%= f.input :contact_first_name, :label => 'Contact first name' %>
  <%= f.input :contact_last_name, :label => 'Contact last name' %>
  <%= f.input :contact_phone_number, :label => 'Contact phone number' %>
  <%= f.input :status, :label => 'Status' %>
  <%= f.input :leased, :label => 'Leased' %>
  <%= f.input :description, :label => 'Description' %>
  <%= f.input :service_id, :label => 'Service', :collection => Service.all %>
<% end %>
<% f.buttons do %>
  <%= f.commit_button %>
<% end %>
<% end %<
