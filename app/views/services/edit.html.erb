<h1>Editing service</h1>

<% form_for(@service) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :descripton %><br />
    <%= f.text_field :descripton %>
  </p>
  <p>
    <%= f.label :fund_to_credit %><br />
    <%= f.text_field :fund_to_credit %>
  </p>
  <p>
    <%= f.label :billing_period %><br />
    <%= f.text_field :billing_period %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @service %> |
<%= link_to 'Back', services_path %>