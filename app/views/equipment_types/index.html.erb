<h1>Listing equipment_types</h1>

<table>
  <tr>
    <th>Description</th>
  </tr>

<% @equipment_types.each do |equipment_type| %>
  <tr>
    <td><%=h equipment_type.description %></td>
    <td><%= link_to 'Show', equipment_type %></td>
    <td><%= link_to 'Edit', edit_equipment_type_path(equipment_type) %></td>
    <td><%= link_to 'Destroy', equipment_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New equipment_type', new_equipment_type_path %>