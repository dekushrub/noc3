<% semantic_form_for @building do |f| %>
<% f.inputs do %>
  <%= f.input :name, :label => 'Name' %>
  <%= f.input :nick, :label => 'Nick' %>
  <%= f.input :address, :label => 'Address' %>
  <%= f.input :city, :label => 'City' %>
  <%= f.input :state, :label => 'State' %>
  <%= f.input :zip, :label => 'Zip' %>
<% end %>
<% f.buttons do %>
  <%= f.commit_button %>
<% end %>
<% end %>
