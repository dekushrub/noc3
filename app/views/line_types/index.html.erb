<h1>Listing Line Types</h1>

<table>
  <tr>
    <th>Description</th>
  </tr>

<% @line_types.each do |line_type| %>
  <tr>
    <td><%=h line_type.description %></td>
    <td><%= link_to 'Show', line_type %></td>
    <td><%= link_to 'Edit', edit_line_type_path(line_type) %></td>
    <td><%= link_to 'Destroy', line_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Line Type', new_line_type_path %>
